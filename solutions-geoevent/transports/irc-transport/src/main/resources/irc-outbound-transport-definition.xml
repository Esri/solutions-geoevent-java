<transport name="IrcOutboundTransport" label="Irc Outbound Transport" contact="geoeventprocessor@esri.com" version="10.2.0" domain="com.esri.geoevent.solutions.transport.irc.outbound" type="outbound">
	<description>Internet Relay Chat (IRC) outbound transport.</description>
		<propertyDefinitions>
		<propertyDefinition propertyName="channelList" label="Channel List"
			description="Comma seperated list of irc channels to listen on."
			propertyType="String" defaultValue="#esriGEP" mandatory="true" readOnly="false" />
		<propertyDefinition propertyName="nickName" label="Nickname"
			description="Nickname to use on IRC channels."
			propertyType="String" defaultValue="EsriGEP" mandatory="true" readOnly="false" />
		<propertyDefinition propertyName="serverName" label="Server Name"
			description="IRC Server"
			propertyType="String" defaultValue="irc.freenode.net" mandatory="true" readOnly="false" />		
	</propertyDefinitions>

</transport>